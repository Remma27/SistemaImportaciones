@model Sistema_de_Gestion_de_Importaciones.Models.Bodega

@{
    ViewData["Title"] = "Editar Bodega";
}

<h1>Editar Bodega</h1>

<h4>Bodega</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div class="form-group">
                <input type="hidden" asp-for="IdBodega" />
                <label asp-for="NombreBodega" class="control-label"></label>
                <input asp-for="NombreBodega" class="form-control" />
                <span asp-validation-for="NombreBodega" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>
